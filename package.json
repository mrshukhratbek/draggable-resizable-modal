{
  "name": "draggable-resizable-modal",
  "version": "0.0.1",
  "description": "",
  "author": "Shukhratbek Kobulov",
  "license": "ISC",
  "main": "./lib/cjs/index.js",
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build"
  },
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "homepage": "https://github.com/mrshukhratbek/react-js-modal#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrshukhratbek/react-js-modal.git"
  },
  "bugs": {
    "url": "https://github.com/mrshukhratbek/react-js-modal/issues"
  },
  "keywords": [
    "draggable-resizable-modal",
    "react-component",
    "component",
    "draggable",
    "resizable",
    "frontend",
    "dialog",
    "modal",
    "react",
    "ui"
  ],
  "devDependencies": {
    "@types/react": "^18.0.21",
    "re-resizable": "^6.9.9",
    "react": "^18.2.0"
  }
}
